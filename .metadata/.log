							pay_main.setBackground(new Color(0, 80, 255));
							pay_btn_group.setBackground(new Color(0, 80, 255));
						}
						pay_price.setText(format.format((item_time / 3600) * 1000) + "");
						Qr_main.setVisible(true);
					}
				}
			});
		}

