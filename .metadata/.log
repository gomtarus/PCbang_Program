	int[] time(int n) { // , ,  
		int h = n / 3600;
		int m = (n / 60) % 60;
		int s = n % 60;
		return new int[] { h, m, s };
	}

	public void Time(int n) { // 
		ActionListener taskPerformer = new ActionListener() {
			public void actionPerformed(ActionEvent evt) {
				if (login_count == 0) {
					left_time--;
					int time[] = time(left_time);
					user_left_time.setText("  : " + time[0] + " " + time[1] + " " + time[2] + " ");
				}

				int start_t[] = time(use_time);
				if (login_count == 0) {
					use_time++;
					user_use_time.setText("  : " + start_t[0] + " " + start_t[1] + " " + start_t[2] + "");
				} else {
					use_time++;
					user_left_time.setText("  : " + start_t[0] + " " + start_t[1] + " " + start_t[2] + "");
				}
			}
		};
		timer = new Timer(1000, taskPerformer);
		timer.setInitialDelay(0);
		if (left_time > 0) {
			timer.start();
		} else {
			timer.stop();
		}
	}

